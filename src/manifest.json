{
  "manifest_version": 2,
  "name": "Chrome.ScheduledBrowsing",
  "description": "Chrome Extension for Scheuled Browsing.",
  "version": "0.1",
  "permissions": [
    "alarms",
    "tabs",
    //"bookmarks",
    "*://*/*"
  ],
  "background": {
    "scripts": ["core.js"],
    "persisent": false
  },
  "browser_action": {
    // "default_icon" : "icon.png",
    "default_title": "Schedule"
  }
}
